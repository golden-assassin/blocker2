{
  "manifest_version": 3,
  "name": "AdBlock PiP",
  "description": "removes Youtube ad elements",
  "version": "1.1.0",
  "content_scripts": [
    {
      "js": [
        "scripts/contents.js",
        "scripts/pip.js"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }
  ]
}
